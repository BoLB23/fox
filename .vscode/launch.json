{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "setup",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["config", "setup", "--verbose"]
    },
    {
      "name": "init",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["init", "--verbose"]
    },
    {
      "name": "build",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": [
        "publish",
        "--verbose",
        "--repo=/home/xadhatter/Workspace/src/github.com/xadhatter-org/hello-world",
        "--skip-deploy"
      ]
    },
    {
      "name": "proxy",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "main.go",
      "args": ["proxy", "--verbose", "8080"]
    }
  ]
}
